{
  "expo": {
    "name": "Frescipe",
    "slug": "frescipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frescipe.app"
    },
    "android": {
      "package": "com.frescipe.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker"
    ]
  }
}
